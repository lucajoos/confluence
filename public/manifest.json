{
  "name": "Snapshot",
  "description": "Save your opened tabs to re-open them whenever you like.",
  "version": "1.2.1",
  "manifest_version": 3,
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "app.html"
  },
  "icons": {
    "16": "icons/png/16x16.png",
    "48": "icons/png/48x48.png",
    "128": "icons/png/128x128.png"
  }
}
